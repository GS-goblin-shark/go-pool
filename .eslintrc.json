{
  "extends": ["airbnb", "airbnb/hooks", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "warn",
    "indent": ["warn", 2],
    "no-unused-vars": ["off", { "vars": "local" }],
    "prefer-const": "warn",
    "quotes": ["warn", "single"],
    "react/prop-types": "off",
    "semi": ["warn", "always"],
    "space-infix-ops": "warn",
    "no-empty": "warn",
    "no-empty-function": "warn"
  }
}
